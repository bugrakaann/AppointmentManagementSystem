@model Models.ViewModel.RegisterViewModel
@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}
<head>
	<title>Register V15</title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<!--===============================================================================================-->
	<link rel="icon" type="image/png" href="~/assets/images/icons/favicon.ico"/>
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="~/assets/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="~/assets/fonts/Linearicons-Free-v1.0.0/icon-font.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="~/assets/vendor/animate/animate.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="~/assets/vendor/css-hamburgers/hamburgers.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="~/assets/vendor/animsition/css/animsition.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="~/assets/vendor/select2/select2.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="~/assets/vendor/daterangepicker/daterangepicker.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="~/assets/css/util.css">
	<link rel="stylesheet" type="text/css" href="~/assets/css/main.css">
	<!--===============================================================================================-->
</head>
<body>
	<div class="limiter">
		<div class="container-login100">
			<div class="wrap-login100">
				<div class="login100-form-title" style="background-image: url(@Url.Content("~/assets/images/bg-01.jpg"));">
					<span class="login100-form-title-1">
						Register
					</span>
				</div>

				<form asp-action="Register" method="post" class="login100-form validate-form">
					<div asp-validation-summary="All" class="text-danger"></div>
					<div class="wrap-input100 validate-input m-b-26" data-validate="Email is required">
						<span class="label-input100">Email</span>
						<input asp-for="Email" class="input100" type="email" placeholder="Enter email">
					</div>

					<div class="wrap-input100 validate-input m-b-18" data-validate="Name is required">
						<span class="label-input100">Full Name</span>
						<input asp-for="FullName" class="input100" type="text" placeholder="Enter name">
					</div>
					<div class="wrap-input100 validate-input m-b-26" data-validate="Username is required">
						<span class="label-input100">Username</span>
						<input asp-for="UserName" class="input100" type="text" placeholder="Enter username">
					</div>

					<div class="wrap-input100 validate-input m-b-18" data-validate="Password is required">
						<span class="label-input100">Password</span>
						<input asp-for="Password" class="input100" type="password" placeholder="Enter password">
					</div>

					<div class="flex-sb-m w-full p-b-30">
						<div class="contact100-form-checkbox">
							<input asp-for="RememberMe" class="input-checkbox100" id="ckb1" type="checkbox">
							<label asp-for="RememberMe" class="label-checkbox100" for="ckb1">
								Remember me
							</label>
						</div>

						<div>
							<a href="#" class="txt1">
								Forgot Password?
							</a>
						</div>
					</div>

					<div class="container-login100-form-btn">
						<button type="submit" class="login100-form-btn">
							Register
						</button>
					</div>
				</form>
			</div>
		</div>
	</div>
	
<!--===============================================================================================-->
	<script src="~/assets/vendor/jquery/jquery-3.2.1.min.js"></script>
<!--===============================================================================================-->
	<script src="~/assets/vendor/animsition/js/animsition.min.js"></script>
<!--===============================================================================================-->
	<script src="~/assets/vendor/bootstrap/js/popper.js"></script>
	<script src="~/assets/css/bootstrap.min.css"></script>
<!--===============================================================================================-->
	<script src="~/assets/vendor/select2/select2.min.js"></script>
<!--===============================================================================================-->
	<script src="~/assets/vendor/daterangepicker/moment.min.js"></script>
	<script src="~/assets/vendor/daterangepicker/daterangepicker.js"></script>
<!--===============================================================================================-->
	<script src="~/assets/vendor/countdowntime/countdowntime.js"></script>
<!--===============================================================================================-->
	<script src="~/assets/js/main.js"></script>
</body>